{
  "name": "nodejs-package-types",
  "version": "1.1.4",
  "description": "Customized Package Types",
  "packageManager": "yarn@3.6.3",
  "main": "index.js",
  "types": "typings/index.d.ts",
  "exports": {
    ".": "./index.js",
    "./hexo": "./hexo",
    "./hexo-log": "./hexo-log",
    "./hexo-bunyan": "./hexo-bunyan",
    "./hexo-util": "./hexo-util",
    "./through2": "./through2",
    "./typings": "./typings",
    "./src": "./src"
  },
  "files": [
    ".gitmodules",
    ".gitattributes",
    "*.json",
    "src",
    "typings",
    "hexo/**/*.{js,ts,json,md}",
    "skeljs/**/*.{js,ts,json,md}",
    "through2/**/*.{js,ts,json,md}",
    "hexo-log/**/*.{js,ts,json,md}",
    "hexo-bunyan/**/*.{js,ts,json,md}",
    "hexo-util/**/*.{js,ts,json,md}",
    "!**/node_modules/**",
    "!**/tmp/**",
    "!**/release/**"
  ],
  "scripts": {
    "update": "npx npm-check-updates -u",
    "pre-build": "git pull -X theirs",
    "build:hexo": "npx fse copy packer.js hexo/packer.js && cd hexo && npm run build",
    "build:through2": "npx fse copy packer.js through2/packer.js && cd through2 && npm run build",
    "build:types": "gulp",
    "build:tarball": "node packer.js && node unused.js",
    "build": "run-s build:**",
    "test": "cd test && npm run update && node setup.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimaslanjaka/nodejs-package-types.git"
  },
  "keywords": [
    "types",
    "typescript",
    "markdown-it-abbr",
    "markdown-it-sup",
    "markdown-it-sub",
    "through2"
  ],
  "author": {
    "email": "dimaslanjaka@gmail.com",
    "name": "L3n40x",
    "url": "https://www.webmanajemen.com/about"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dimaslanjaka/nodejs-package-types/issues"
  },
  "homepage": "https://github.com/dimaslanjaka/nodejs-package-types#readme",
  "devDependencies": {
    "@atao60/fse-cli": "^0.1.9",
    "@types/fs-extra": "^11.0.4",
    "@types/gulp": "^4.0.17",
    "@types/markdown-it": "14.1.2",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "binary-collections": "^2.0.9",
    "browser-sync": "^3.0.4",
    "depcheck": "^1.4.7",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "express": "^5.1.0",
    "git-command-helper": "^2.1.0",
    "glob": "^11.0.3",
    "gulp": "^5.0.1",
    "markdown-it": "^14.1.0",
    "markdown-it-abbr": "^2.0.0",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-attrs": "^4.3.1",
    "markdown-it-footnote": "^4.0.0",
    "markdown-it-mark": "^4.0.0",
    "markdown-it-sub": "^2.0.0",
    "markdown-it-sup": "^2.0.0",
    "npm-check-updates": "^18.0.2",
    "prettier": "^3.6.2",
    "semver": "^7.7.2",
    "slugify": "^1.6.6",
    "typedoc": "^0.28.7",
    "typedoc-plugin-missing-exports": "^4.0.0"
  },
  "dependencies": {
    "@types/express": "^5.0.3",
    "@types/through2": "./through2",
    "ansi-colors": "^4.1.3",
    "axios": "^1.11.0",
    "cross-spawn": "https://github.com/dimaslanjaka/node-cross-spawn/raw/private/release/cross-spawn.tgz",
    "fs-extra": "^11.3.0",
    "hexo-util": "^3.3.0",
    "sbg-utility": "^2.0.8",
    "through2": "^4.0.2",
    "true-case-path": "^2.2.1",
    "upath": "^2.0.1"
  },
  "resolutions": {
    "sbg-utility": "https://github.com/dimaslanjaka/static-blog-generator/raw/df939d4cb3e31fded39412534bcef11de9fe0dcc/packages/sbg-utility/release/sbg-utility.tgz",
    "git-command-helper": "https://github.com/dimaslanjaka/git-command-helper/raw/40f673205b55bd451a689079b47fde804ce14559/release/git-command-helper.tgz",
    "cross-spawn": "https://github.com/dimaslanjaka/node-cross-spawn/raw/80999ac7a7031a81e11ddb5fab0f8ba433041c98/release/cross-spawn.tgz"
  }
}

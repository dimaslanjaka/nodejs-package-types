{
  "name": "nodejs-package-types",
  "version": "1.1.2",
  "description": "Customized Package Types",
  "packageManager": "yarn@3.2.0",
  "main": "index.js",
  "types": "typings/index.d.ts",
  "files": [
    ".github",
    ".gitmodules",
    "*.js",
    "*.json",
    "src",
    "typings",
    "hexo/**/*.{js,ts,json,md}",
    "skeljs/**/*.{js,ts,json,md}",
    "through2/**/*.{js,ts,json,md}",
    "hexo-log/**/*.{js,ts,json,md}",
    "hexo-bunyan/**/*.{js,ts,json,md}",
    "hexo-util/**/*.{js,ts,json,md}"
  ],
  "scripts": {
    "update": "npx npm-check-updates -u && npm install",
    "build": "node packer.js && node unused.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimaslanjaka/nodejs-package-types.git"
  },
  "keywords": [
    "types",
    "typescript",
    "markdown-it-abbr",
    "markdown-it-sup",
    "markdown-it-sub",
    "through2"
  ],
  "author": {
    "email": "dimaslanjaka@gmail.com",
    "name": "L3n40x",
    "url": "https://www.webmanajemen.com/about"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dimaslanjaka/nodejs-package-types/issues"
  },
  "homepage": "https://github.com/dimaslanjaka/nodejs-package-types#readme",
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/gulp": "^4.0.10",
    "@types/node": "^18.11.12",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.46.0",
    "cross-spawn": "^7.0.3",
    "depcheck": "^1.4.3",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gulp": "^4.0.2",
    "npm-check-updates": "^16.5.6",
    "prettier": "^2.8.1"
  },
  "dependencies": {
    "fs-extra": "^11.1.0",
    "upath": "^2.0.1"
  }
